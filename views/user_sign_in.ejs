<link rel="stylesheet" href="/css/user_sign_in.css">

<div id="sign-in-container">
    <div id="simple-sign-in">
        <h1>Sign In </h1>

        <form id="sign-in-form" action="/users/create-session" method="POST"> 
            <p>Email</p>
            <input type="email" name="email" placeholder="Email"  required><br>
            <p>Password</p>
            <input type="password" name="password" placeholder="Password"  required><br> <br> <br>
            <input id="submit" type="submit" value="Sign In">

        </form>
    </div>

    <div id="google-sign-in">
        <p>
            <h3>
                <a href="/users/auth/google"> Sign In using Google</a>
            </h3>
        </p>

    </div>

</div>
        

        